<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ambulancias_repostaje_form" model="ir.ui.view">
            <field name="name">ambulancias.repostaje.form</field>
            <field name="model">ambulancias.repostaje</field>
            <field name="arch" type="xml">
                <form string="Repostaje">
                    <sheet>
                        <group>
                            <field name="km"/>
                            <field name="fecha"/>
                            <field name="litros"/>
                            <field name="importe_repostado"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ambulancias_repostaje_tree" model="ir.ui.view">
            <field name="name">ambulancias.repostaje.tree</field>
            <field name="model">ambulancias.repostaje</field>
            <field name="arch" type="xml">
                <tree string="Repostaje">
                    <field name="km"/>
                    <field name="fecha"/>
                    <field name="litros"/>
                    <field name="importe_repostado"/>
                </tree>
            </field>
        </record>

        <record id="ambulancias_repostaje_search" model="ir.ui.view">
            <field name="name">ambulancias.repostaje.search</field>
            <field name="model">ambulancias.repostaje</field>
            <field name="arch" type="xml">
                <search string="Repostaje">

                </search>
            </field>
        </record>

        <record id="ambulancias.repostajes_action_window" model="ir.actions.act_window">
            <field name="name">Repostaje</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ambulancias.repostaje</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Vehiculo.
                </p>
            </field>
        </record>
    </data>
</odoo>