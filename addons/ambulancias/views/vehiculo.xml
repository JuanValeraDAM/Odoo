<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ambulancias_vehiculo_form" model="ir.ui.view">
            <field name="name">ambulancias.vehiculo.form</field>
            <field name="model">ambulancias.vehiculo</field>
            <field name="arch" type="xml">
                <form string="Vehiculo">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="num_bastidor"/>
                            <field name="fecha_adquisicion"/>
                            <field name="fecha_baja"/>
                            <field name="fecha_proximo_mantenimiento"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ambulancias_vehiculo_tree" model="ir.ui.view">
            <field name="name">ambulancias.vehiculo.tree</field>
            <field name="model">ambulancias.vehiculo</field>
            <field name="arch" type="xml">
                <tree string="Vehiculo">
                    <field name="name"/>
                    <field name="num_bastidor"/>
                    <field name="fecha_adquisicion"/>
                    <field name="fecha_baja"/>
                    <field name="fecha_proximo_mantenimiento"/>
                </tree>
            </field>
        </record>

        <record id="ambulancias_vehiculo_search" model="ir.ui.view">
            <field name="name">ambulancias.vehiculo.search</field>
            <field name="model">ambulancias.vehiculo</field>
            <field name="arch" type="xml">
                <search string="Vehiculo">

                </search>
            </field>
        </record>

        <record id="ambulancias.vehiculos_action_window" model="ir.actions.act_window">
            <field name="name">Vehiculo</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ambulancias.vehiculo</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>